<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="bp" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pe.edu.upeu.library.crudlibrary.controller.controllerMainMenu">
    <top>
        <MenuBar fx:id="menuBar" BorderPane.alignment="CENTER">
            <menus>
                <Menu fx:id="menu1" mnemonicParsing="false" text="Participante">
                    <items>
                        <MenuItem fx:id="menuItem1" mnemonicParsing="false" text="Principal" />
                        <MenuItem fx:id="menuItem2" mnemonicParsing="false" text="Prestamos" />
                        <MenuItem fx:id="menuItem3" mnemonicParsing="false" text="Devoluciones" />
                        <MenuItem fx:id="menuItem4" mnemonicParsing="false" text="Usuarios" />
                        <MenuItem fx:id="menuItem5" mnemonicParsing="false" text="Libros" />
                        <MenuItem fx:id="menuItem6" mnemonicParsing="false" text="Reportes" />
                        <MenuItem fx:id="menuItemc" mnemonicParsing="false" text="Salir" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
    <center>
        <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER"/>
    </center>
</BorderPane>
